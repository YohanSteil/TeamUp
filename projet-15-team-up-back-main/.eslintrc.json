{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    // "eslint-disable": "linebreak-style"
    "import/extensions": "off",
    "comma-dangle": ["error", "only-multiline"],
    "object-curly-newline": [
      "error",
      { "multiline": true, "consistent": true }
    ],

    "import/prefer-default-export": "off",
    "quotes": "off"
  }
}
